<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".driver.DriverMainActivity">

    <include
        android:id="@+id/activity_driver_main_toolbar"
        layout="@layout/toolbar_driver" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/driver_fragment_frame"
        android:name="com.example.shuttlemobile.driver.DriverMainFragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/activity_driver_main_toolbar"
        tools:layout="@layout/fragment_driver_ride_history" />

</androidx.constraintlayout.widget.ConstraintLayout>