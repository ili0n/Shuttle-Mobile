<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".driver.DriverRideHistoryItemActivity">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:background="#202020"
        android:src="@drawable/ic_baseline_message_24"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="#393939"
        android:src="@drawable/ic_baseline_history_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/driver_ride_history_list_toolbar" />

    <TextView
        android:id="@+id/driver_ride_history_item_lbl_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Passengers"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/driver_ride_history_item_lbl_7"
        app:layout_constraintTop_toTopOf="@+id/imageView6" />

    <TextView
        android:id="@+id/driver_ride_history_item_end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12/12/2022 11:59"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/driver_ride_history_item_lbl_2"
        app:layout_constraintTop_toTopOf="@+id/driver_ride_history_item_lbl_2" />

    <TextView
        android:id="@+id/driver_ride_history_item_lbl_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Distance"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/driver_ride_history_item_lbl_2"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <TextView
        android:id="@+id/driver_ride_history_item_begin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12/12/2022 11:23"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/driver_ride_history_item_lbl_1"
        app:layout_constraintTop_toTopOf="@+id/driver_ride_history_item_lbl_1" />

    <Button
        android:id="@+id/driver_ride_history_item_inbox"
        android:layout_width="91dp"
        android:layout_height="48dp"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:text="Chat"
        app:layout_constraintStart_toStartOf="@+id/driver_ride_history_item_lbl_3"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />

    <TextView
        android:id="@+id/driver_ride_history_item_lbl_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="End"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/driver_ride_history_item_lbl_1"
        app:layout_constraintTop_toBottomOf="@+id/driver_ride_history_item_lbl_1" />

    <TextView
        android:id="@+id/driver_ride_history_item_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="138,00 RSD"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/driver_ride_history_item_lbl_5"
        app:layout_constraintTop_toTopOf="@+id/driver_ride_history_item_lbl_5" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:background="#1C1C1C"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <TextView
        android:id="@+id/driver_ride_history_item_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/driver_ride_history_list_toolbar" />

    <TextView
        android:id="@+id/driver_ride_history_item_lbl_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rating"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/driver_ride_history_item_lbl_5"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/driver_ride_history_item_lbl_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Begin"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <include
        android:id="@+id/driver_ride_history_list_toolbar"
        layout="@layout/toolbar_driver"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:background="#393939"
        android:src="@drawable/ic_baseline_account_circle_24"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:srcCompat="@drawable/ic_baseline_rate_review_24" />

    <TextView
        android:id="@+id/driver_ride_history_item_lbl_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Comment"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/driver_ride_history_item_lbl_6"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:background="#2C2C2C"
        android:src="@drawable/ic_baseline_attach_money_24"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:background="#393939"
        android:src="@drawable/ic_baseline_account_circle_24"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:background="#2C2C2C"
        android:src="@drawable/ic_baseline_attach_money_24"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <TextView
        android:id="@+id/driver_ride_history_item_lbl_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Price"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/driver_ride_history_item_lbl_4"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <TextView
        android:id="@+id/driver_ride_history_item_distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1.2 km"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/driver_ride_history_item_lbl_4"
        app:layout_constraintTop_toTopOf="@+id/driver_ride_history_item_lbl_4" />

    <TextView
        android:id="@+id/driver_ride_history_item_rating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5/5"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/driver_ride_history_item_lbl_6"
        app:layout_constraintTop_toTopOf="@+id/driver_ride_history_item_lbl_6" />

    <ScrollView
        android:layout_width="224dp"
        android:layout_height="80dp"
        android:clipToPadding="true"
        android:fillViewport="true"
        android:scrollbarStyle="outsideOverlay"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/driver_ride_history_item_lbl_7"
        app:layout_constraintTop_toTopOf="@+id/driver_ride_history_item_lbl_7">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/driver_ride_history_item_comment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="HAJKSAHSKJAHDKJSDHSKJDHASKJSDHAKJSHAKDJASHKDJSHDKJAH" />
        </LinearLayout>
    </ScrollView>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:background="#292929"
        android:src="@drawable/ic_baseline_route_24"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Route"
        app:layout_constraintStart_toStartOf="@+id/driver_ride_history_item_inbox"
        app:layout_constraintTop_toTopOf="@+id/imageView4" />

    <ListView
        android:id="@+id/driver_ride_history_item_routes"
        android:layout_width="227dp"
        android:layout_height="67dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/textView" />

</androidx.constraintlayout.widget.ConstraintLayout>